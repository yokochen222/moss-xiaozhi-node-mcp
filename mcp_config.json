{
  "mcpServers": {
    "yo-onvif-mcp": {
      "command": "npx",
      "args": ["yo-onvif-mcp"],
      "env": {
        "ONVIF_USERNAME": "admin",
        "ONVIF_PASSWORD": "admin123",
        "ONVIF_XADDR": "http://192.168.31.10:80/onvif/device_service"
      }
    },
    "yo-execute-shortcuts-mcp": {
      "command": "npx",
      "args": ["yo-execute-shortcuts-mcp", "--config", "yo-execute-shortcuts-mcp.json"]
    }
  }
}
