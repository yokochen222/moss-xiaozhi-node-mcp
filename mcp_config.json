{
  "mcpServers": {
    "shortcuts-mcp": {
      "type": "stdio",
      "command": "tsx",
      "args": ["src/shortcuts/index.ts"],
      "disabled": false
    },
    "remote-sse-server": {
      "type": "sse",
      "url": "https://api.example.com/sse",
      "disabled": true
    },
    "remote-http-server": {
      "type": "http",
      "url": "https://api.example.com/mcp",
      "disabled": true
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest"],
      "disabled": true
    }
  }
}
